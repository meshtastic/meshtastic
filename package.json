    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "katex": "^0.16.25",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-accessible-accordion": "^5.0.1",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-player": "^3.4.0",
    "remark-deflist": "^1.0.0",
    "swr": "^2.3.6",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tsconfig/docusaurus": "^2.0.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "docusaurus-prince-pdf": "^1.2.1",
    "husky": "^9.1.0",
    "lint-staged": "^16.0.0",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.41.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "oxlint",
      "oxfmt --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "path-to-regexp": "^3.3.0",
      "cookie": "^0.7.0",
      "prismjs": "^1.30.0"
    },
    "ignoredBuiltDependencies": [
      "core-js",
      "core-js-pure"
    ]
  }
